{
  "name": "aq-miniapp-base",
  "version": "0.0.1",
  "description": "AQSoft MiniApp Base code",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run babel",
    "babel": "babel ./src --out-dir=./build",
    "watchz": "babel --watch ./src --out-dir=./build",
    "watch": "npm-watch",
    "copycss": "rm -rf build/css && cp -R src/css build/css/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/aqsoftware/aq-miniapp-base.git"
  },
  "author": "Ryan C. Brozo",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/aqsoftware/aq-miniapp-base#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "flow-bin": "^0.47.0",
    "npm-watch": "^0.1.9",
    "postcss-loader": "^2.0.5",
    "style-loader": "^0.18.2"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "query-string": "^4.3.4",
    "react": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "peerDependencies": {
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ]
      }
    }
  },
  "watch": {
    "babel": {
      "patterns": [
        "src",
        "src/js"
      ],
      "extensions": "js,jsx"
    },
    "copycss": {
      "patterns": [
        "src/css"
      ],
      "extensions": "css"
    }
  }
}
